{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "lib": ["es5", "es6", "dom"],
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "Assets": ["assets"],
      "Components": ["components"]
    }
  },
  "include": ["./src/**/*"],
  "awesomeTypescriptLoaderOptions": {
    "reportFiles": ["./src/**/*"]
  }
}
